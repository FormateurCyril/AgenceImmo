<table align="center" border="0" cellpadding="0" callspacing="0" role="presentation" style="vertical-align:top;width='100%';">
<tr>
    <td>Bien visité : </td>
    <td><a href="{{ url('property.show', {id: contact.property.id, slug: contact.property.slug}) }}">{{ contact.property.title }}</a></td>
</tr>
<tr>
    <td>Prénom</td>
    <td>{{ contact.firstname }}</td>
</tr>
<tr>
    <td>Nom</td>
    <td>{{ contact.lastname }}</td>
</tr>
<tr>
    <td>Téléphone</td>
    <td>{{ contact.phone }}</td>
</tr>
<tr>
    <td>Email</td>
    <td>{{ contact.email }}</td>
</tr>

</table>